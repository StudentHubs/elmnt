{
  "name": "elmnt",
  "version": "0.1.3",
  "description": "HTML elements reimagined with react",
  "author": "Jon Whitehead",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib/*",
    "watch": "yarn run clean && tsc -w",
    "build": "yarn run clean && tsc",
    "test": "jest",
    "prepublish": "yarn run test && yarn run build",
    "dev": "webpack-dev-server"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "devDependencies": {
    "@types/enzyme": "^2.8.0",
    "@types/jest": "^19.2.3",
    "@types/react-dom": "^15.5.0",
    "enzyme": "^2.8.2",
    "jest": "^20.0.4",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-test-renderer": "^15.5.4",
    "rimraf": "^2.6.1",
    "source-map-loader": "^0.2.1",
    "ts-jest": "^20.0.5",
    "ts-loader": "^2.1.0",
    "typescript": "^2.3.4",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
  "peerDependencies": {
    "react": "^15.0.0"
  },
  "dependencies": {
    "@types/react": "^15.0.25",
    "@types/recompose": "^0.22.0",
    "highstyle": "^0.2.4",
    "mishmash": "^0.2.5",
    "most": "^1.4.0",
    "recompose": "^0.23.4"
  }
}
