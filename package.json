{
  "name": "elmnt",
  "version": "0.2.4",
  "description": "HTML elements reimagined with react",
  "author": "Jon Whitehead",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "prettier": "prettier --single-quote --trailing-comma all --write \"{src,test}/**/*.{ts,tsx}\"",
    "clean": "rimraf lib/*",
    "watch": "yarn run clean && tsc -w",
    "build": "yarn run clean && tsc",
    "test": "jest",
    "prepare": "yarn run test && yarn run build",
    "dev": "webpack-dev-server"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": ["ts", "tsx", "js"]
  },
  "devDependencies": {
    "@types/enzyme": "^2.8.5",
    "@types/jest": "^20.0.6",
    "@types/react-dom": "^15.5.2",
    "enzyme": "^2.9.1",
    "jest": "^20.0.4",
    "prettier": "^1.5.3",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.1",
    "rimraf": "^2.6.1",
    "source-map-loader": "^0.2.1",
    "ts-jest": "^20.0.10",
    "ts-loader": "^2.3.2",
    "typescript": "^2.4.2",
    "webpack": "^3.5.3",
    "webpack-dev-server": "^2.7.1"
  },
  "peerDependencies": {
    "react": "^15.0.0"
  },
  "dependencies": {
    "@types/react": "^16.0.2",
    "@types/recompose": "^0.23.3",
    "mishmash": "^0.3.3",
    "most": "^1.5.1",
    "recompose": "^0.24.0"
  }
}
