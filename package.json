{
  "name": "elmnt",
  "version": "0.3.6",
  "description": "HTML elements reimagined with react",
  "author": "Jon Whitehead",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "prettier":
      "prettier --single-quote --trailing-comma all --write \"{src,test}/**/*.{ts,tsx}\"",
    "clean": "rimraf lib/*",
    "watch": "yarn run clean && tsc -w",
    "build": "yarn run clean && tsc",
    "test": "jest",
    "prepare": "yarn run test && yarn run build",
    "dev": "webpack-dev-server"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": ["ts", "tsx", "js"],
    "setupFiles": ["./test/setup.ts"],
    "snapshotSerializers": ["enzyme-to-json/serializer"]
  },
  "devDependencies": {
    "@types/enzyme": "^2.8.12",
    "@types/jest": "^21.1.2",
    "@types/node": "^8.0.34",
    "@types/react-dom": "^16.0.1",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "enzyme-to-json": "^3.1.2",
    "jest": "^21.2.1",
    "prettier": "^1.7.4",
    "raf": "^3.4.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-test-renderer": "^16.0.0",
    "rimraf": "^2.6.2",
    "source-map-loader": "^0.2.2",
    "ts-jest": "^21.1.2",
    "ts-loader": "^2.3.7",
    "typescript": "^2.5.3",
    "webpack": "^3.7.1",
    "webpack-dev-server": "^2.9.1"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "dependencies": {
    "@types/react": "^16.0.10",
    "@types/recompose": "^0.24.2",
    "commonmark": "^0.28.1",
    "commonmark-react-renderer": "^4.3.3",
    "mishmash": "^0.4.3",
    "most": "^1.7.2",
    "recompose": "^0.26.0"
  }
}
