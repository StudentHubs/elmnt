{
  "name": "elmnt",
  "version": "0.5.2",
  "description": "HTML elements reimagined with react",
  "author": "Jon Whitehead",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "prettier":
      "prettier --single-quote --trailing-comma all --write \"{src,test}/**/*.{ts,tsx}\"",
    "clean": "rimraf lib/*",
    "watch": "yarn run clean && tsc -w",
    "build": "yarn run clean && tsc",
    "test": "jest",
    "prepare": "yarn run test && yarn run build",
    "dev": "webpack-dev-server",
    "docs": "webpack --config ./docs/webpack.config.js"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": ["ts", "tsx", "js"],
    "setupFiles": ["./test/setup.ts"],
    "snapshotSerializers": ["enzyme-to-json/serializer"]
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.9",
    "@types/jest": "^22.1.3",
    "@types/node": "^9.4.6",
    "@types/react-dom": "^16.0.4",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "jest": "^22.4.2",
    "prettier": "^1.10.2",
    "raf": "^3.4.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0",
    "rimraf": "^2.6.2",
    "source-map-loader": "^0.2.3",
    "ts-jest": "^22.0.4",
    "ts-loader": "^3.5.0",
    "typescript": "^2.7.2",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.1"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "dependencies": {
    "@types/react": "^16.0.38",
    "commonmark": "^0.28.1",
    "commonmark-react-renderer": "^4.3.4",
    "mishmash": "^0.7.0",
    "style-transform": "^0.1.11"
  }
}
