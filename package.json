{
  "name": "elmnt",
  "version": "0.2.0",
  "description": "HTML elements reimagined with react",
  "author": "Jon Whitehead",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "prettier": "prettier --single-quote --trailing-comma all --write \"{src,test}/**/*.{ts,tsx}\"",
    "clean": "rimraf lib/*",
    "watch": "yarn run clean && tsc -w",
    "build": "yarn run clean && tsc",
    "test": "jest",
    "prepublish": "yarn run test && yarn run build",
    "dev": "webpack-dev-server"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "devDependencies": {
    "@types/enzyme": "^2.8.0",
    "@types/jest": "^20.0.1",
    "@types/react-dom": "^15.5.0",
    "enzyme": "^2.9.0",
    "jest": "^20.0.4",
    "prettier": "^1.4.4",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.1",
    "rimraf": "^2.6.1",
    "source-map-loader": "^0.2.1",
    "ts-jest": "^20.0.6",
    "ts-loader": "^2.2.0",
    "typescript": "2.3.4",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0"
  },
  "peerDependencies": {
    "react": "^15.0.0"
  },
  "dependencies": {
    "@types/react": "^15.0.30",
    "@types/recompose": "^0.23.0",
    "highstyle": "^0.2.6",
    "mishmash": "^0.2.7",
    "most": "^1.4.1",
    "recompose": "^0.23.5"
  }
}
